<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintWidth_percent="1"
        app:layout_constraintDimensionRatio="1">
        <ImageView
            android:id="@+id/diaryImage"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:src="@drawable/diary_text"/>
        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Title"
            android:textSize="40dp"
            app:layout_constraintLeft_toLeftOf="@id/diaryImage"
            app:layout_constraintRight_toRightOf="@id/diaryImage"
            app:layout_constraintTop_toTopOf="@id/diaryImage"
            app:layout_constraintBottom_toBottomOf="@id/diaryImage"
            android:gravity="center"
            app:layout_constraintVertical_bias="0.35"/>
        <FrameLayout
            android:id="@+id/diaryCoverNextWriterIcon"
            android:layout_width="0dp"
            android:layout_height="0dp"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintWidth_percent="0.3"
            app:layout_constraintDimensionRatio="1"
            app:layout_constraintVertical_bias="0.67">
            <include
                android:id="@+id/diary_cover_icon"
                layout="@layout/user_character"/>
        </FrameLayout>

        <FrameLayout
            android:id="@+id/diaryCoverFirstOrderIcon"
            android:layout_width="0dp"
            android:layout_height="0dp"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintWidth_percent="0.2"
            app:layout_constraintDimensionRatio="1"
            app:layout_constraintVertical_bias="0.95"
            app:layout_constraintHorizontal_bias="0.22">
            <include
                android:id="@+id/diary_cover_icon"
                layout="@layout/user_character"/>
        </FrameLayout>

        <FrameLayout
            android:id="@+id/diaryCoverSecondOrderIcon"
            android:layout_width="0dp"
            android:layout_height="0dp"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintWidth_percent="0.2"
            app:layout_constraintDimensionRatio="1"
            app:layout_constraintVertical_bias="0.95"
            app:layout_constraintHorizontal_bias="0.4">
            <include
                android:id="@+id/diary_cover_icon"
                layout="@layout/user_character"/>
        </FrameLayout>

        <FrameLayout
            android:id="@+id/diaryCoverThirdOrderIcon"
            android:layout_width="0dp"
            android:layout_height="0dp"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintWidth_percent="0.2"
            app:layout_constraintDimensionRatio="1"
            app:layout_constraintVertical_bias="0.95"
            app:layout_constraintHorizontal_bias="0.57">
            <include
                android:id="@+id/diary_cover_icon"
                layout="@layout/user_character"/>
        </FrameLayout>

        <FrameLayout
            android:id="@+id/diaryCoverFifthOrderIcon"
            android:layout_width="0dp"
            android:layout_height="0dp"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintWidth_percent="0.2"
            app:layout_constraintDimensionRatio="1"
            app:layout_constraintVertical_bias="0.95"
            app:layout_constraintHorizontal_bias="0.74">
            <include
                android:id="@+id/diary_cover_icon"
                layout="@layout/user_character"/>
        </FrameLayout>

    </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.constraintlayout.widget.ConstraintLayout>